[loadbalancers]
default ansible_ssh_host=127.0.0.1

[loadbalancers:vars]
ansible_ssh_port=2222
ansible_ssh_user=vagrant
ansible_ssh_private_key_file=~/.vagrant.d/insecure_private_key

[databases]
default ansible_ssh_host=127.0.0.1

[databases:vars]
ansible_ssh_port=2222
ansible_ssh_user=vagrant
ansible_ssh_private_key_file=~/.vagrant.d/insecure_private_key

[webservers]
default ansible_ssh_host=127.0.0.1

[webservers:vars]
ansible_ssh_port=2222
ansible_ssh_user=vagrant
ansible_ssh_private_key_file=~/.vagrant.d/insecure_private_key
